<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/constraintLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.assessment.EditAssessmentFragment">

    <fragment
        android:id="@+id/assessmentPropertiesFragment"
        android:name="Erwine.Leonard.T.wguscheduler356334.ui.assessment.AssessmentPropertiesFragment"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toTopOf="@+id/shareImageButton"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout="@layout/fragment_assessment_properties" />

    <ImageButton
        android:id="@+id/shareImageButton"
        style="@style/WGU.Scheduler.ShareImageButtonStyle"
        android:contentDescription="@string/label_share_assessment"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/saveImageButton" />

    <ImageButton
        android:id="@+id/saveImageButton"
        style="@style/WGU.Scheduler.SaveImageButtonStyle"
        android:contentDescription="@string/command_save_changes"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/deleteImageButton" />

    <ImageButton
        android:id="@+id/deleteImageButton"
        style="@style/WGU.Scheduler.DeleteImageButtonStyle"
        android:contentDescription="@string/command_delete"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/cancelImageButton" />

    <ImageButton
        android:id="@+id/cancelImageButton"
        style="@style/WGU.Scheduler.CancelImageButtonStyle"
        android:contentDescription="@android:string/cancel"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>